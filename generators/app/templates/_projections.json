{
    "test/integration/*-spec.js": {
        "alternate": "src/{}.js",
        "type": "integration-test"
    },
    "test/unit/*-spec.js": {
        "alternate": "src/{}.js",
        "type": "unit-test"
    },
    "src/*.js": {
        "alternate": "test/unit/{}-spec.js",
        "type": "src"
    }
}
